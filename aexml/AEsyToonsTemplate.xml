<?xml version="1.0" encoding="UTF-8"?>
	<templates>
	<template name="AEsyToons">
		<label>syToons</label>
		<description>...</description>
		<attribute name="engine" type="maya.enum">
			<label>Engine</label>
			<description></description>
		</attribute>
		<attribute name="color_major" type="maya.float3">
			<label>Major Color</label>
			<description></description>
		</attribute>
		<attribute name="color_shadow" type="maya.float3">
			<label>Shadow Color</label>
			<description></description>
		</attribute>
		<attribute name="color_mask" type="maya.float3">
			<label>Mask Color</label>
			<description></description>
		</attribute>
		<attribute name="color_outline" type="maya.float3">
			<label>Outline Color</label>
			<description></description>
		</attribute>
		<attribute name="lambert_color" type="maya.float3">
			<label>Lambert Color</label>
			<description></description>
		</attribute>
		<attribute name="shadow_ramp" type="maya.float3">
			<label>Shadow Ramp</label>
			<description></description>
		</attribute>
		<attribute name="shadow_position" type="maya.float">
			<label>Shadow Position</label>
			<description></description>
		</attribute>
		<attribute name="casting_light" type="maya.bool">
			<label>Casting Light</label>
			<description></description>
		</attribute>
		<attribute name="casting_occlusion" type="maya.bool">
			<label>Casting Occlusion</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_sytoons_beauty" type="maya.float3">
			<label>Aov syToons Beauty</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_color_major" type="maya.float3">
			<label>Aov Color Major</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_color_shadow" type="maya.float3">
			<label>Aov Color Shadow</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_color_mask" type="maya.float3">
			<label>Aov Color Mask</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_outline" type="maya.float3">
			<label>Aov Outline</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_dynamic_shadow" type="maya.float3">
			<label>Aov Dynamic Shadow</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_dynamic_shadow_raw" type="maya.float3">
			<label>Aov Dynamic Shadow Raw</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_normal" type="maya.float3">
			<label>Aov Normal</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_fresnel" type="maya.float3">
			<label>Aov Fresnel</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_depth" type="maya.float3">
			<label>Aov Depth</label>
			<description></description>
		</attribute>
		<attribute name="sy_aov_occlusion" type="maya.float3">
			<label>Aov Occlusion</label>
			<description></description>
		</attribute>

	</template>
		<view name="Lookdev" template="AEsyToons">
			<property name="engine"/>
			<group name="TextureShading">
				<label>Texture Shading</label>
			<property name="color_major"/>
			<property name="color_shadow"/>
			<property name="color_mask"/>
			<property name="color_outline"/>
			</group>
			<group name="LightShading">
				<label>Light Shading</label>
			<property name="lambert_color"/>
			<property name="shadow_ramp"/>
			<property name="shadow_position"/>
			</group>
			<group name="Optimization">
				<label>Light Shading</label>
			<property name="casting_light"/>
			<property name="casting_occlusion"/>
			</group>
			<group name="AOVs">
				<label>AOVs</label>
			<property name="sy_aov_sytoons_beauty"/>
			<property name="sy_aov_color_major"/>
			<property name="sy_aov_color_shadow"/>
			<property name="sy_aov_color_mask"/>
			<property name="sy_aov_outline"/>
			<property name="sy_aov_dynamic_shadow"/>
			<property name="sy_aov_dynamic_shadow_raw"/>
			<property name="sy_aov_normal"/>
			<property name="sy_aov_fresnel"/>
			<property name="sy_aov_depth"/>
			<property name="sy_aov_occlusion"/>
			</group>
		</view>
</templates>
